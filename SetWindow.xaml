<Window x:Class="_13.SetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_13"
        mc:Ignorable="d"
        Title="SetWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Изменить приоритет" HorizontalAlignment="Center"></TextBlock>
            <TextBox Name="TBPriority" Width="150" Height="20" FontSize="15"></TextBox>
            <Button Content="Изменить"  Name="SetPriority" Width="140" Height="27" Click="SetPriority_Click"></Button>
        </StackPanel>
    </Grid>
    </Window>
